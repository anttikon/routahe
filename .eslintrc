{
  "root": true,
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-unexpected-multiline": 2,
    "semi": [1, "never"],
    "no-unused-vars": [1, { "vars": "all", "args": "none" }],
    "no-var": 1,
    "eqeqeq": 1,
    "dot-notation": 1,
    "no-caller": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-implied-eval": 2,
    "strict": 0,
    "no-multi-spaces": 1,
    "no-console": 0,
    "no-case-declarations": 0
  },
  "globals": {
    "__filename": true
  }
}